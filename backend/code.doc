CREATE TABLE `barbarshop`.`appointments` (
  `id` INT NOT NULL AUTO_INCREMENT,
  `firstName` VARCHAR(45) NULL,
  `lastName` VARCHAR(45) NULL,
  `email` VARCHAR(100) NULL,
  `phone` VARCHAR(15) NULL,
  `date` DATE NULL,
  `description` LONGTEXT NULL,
  PRIMARY KEY (`id`));



